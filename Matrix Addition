// Matrix Addition

#include <stdio.h>

int main() 
{
    int a[2][3], b[2][3];
    printf("First Array\n");
    for(int row=0;row<2;row++)
    {
     for(int col=0;col<3;col++)
     {
      scanf("%d",&a[row][col]);
     }
    }
    
    printf("Second Array\n");
    for(int row=0;row<2;row++)
    {
     for(int col=0;col<3;col++)
     {
      scanf("%d",&b[row][col]);
     }
    }
    printf("Added Array\n");
    for(int row=0;row<2;row++)
    {
     for(int col=0;col<3;col++)
     {
      printf("%d ",a[row][col]+b[row][col]);
     }
     printf("\n");
        
    }
        return 0;
}

***********************************************************************
Output
First Array
1 2 5 3 4 6
Second Array
2 1 3 3 1 2
Added Array
3 3 8 
6 5 8 
