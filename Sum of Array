// C Program to Find Sum of Array Elements 

#include <stdio.h>

void main()
{
    int n,sum=0;
    printf("Enter the count");
    scanf("%d",&n);
    
    int num[n];
    
    for(int i=0;i<n;i++)
    {
        printf("\nEnter Element %d : ",i+1);
        scanf("%d",&num[i]);
        sum = sum + num[i];
    }
    
    printf("\nSum of the array is %d ",sum);
}
